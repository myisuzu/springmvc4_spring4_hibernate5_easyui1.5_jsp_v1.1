<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.myframework.common.domain">
	
	<class name="CommonFile" table="t_common_file">
		<!-- 公共字段 -->
		<id name="id">
			<generator class="native" />
		</id>
		<property name="sort" update="false"/>
		<property name="enable" update="false"/>
		<property name="addDate" column="add_date" update="false"/>
		<property name="updateDate" column="update_date" />
		<property name="remark" type="text"/>
		<!-- 公共字段 -->
		
		<property name="url"/>
		<property name="name"/>
		<property name="size"/>
		<property name="type"/>
	</class>

</hibernate-mapping>